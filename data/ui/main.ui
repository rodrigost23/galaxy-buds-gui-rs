  <object class="AdwWindow" id="main_window">
    <child>
      <object class="AdwNavigationSplitView" id="split_view">
        <property name="content">
          <object class="AdwNavigationPage">
            <child>
              <object class="AdwToolbarView">
                <child type="top">
                  <object class="AdwHeaderBar"/>
                </child>
                <child>
                  <object class="AdwViewStack" id="view_stack">
                    <child>
                      <object class="AdwViewStackPage" id="page_summary">
                        <property name="child">
                          <object class="GtkLabel">
                            <property name="label">Summary</property>
                          </object>
                        </property>
                      </object>
                    </child>
                    <child>
                      <object class="AdwViewStackPage" id="page_option_1">
                        <property name="child">
                          <object class="GtkLabel">
                            <property name="label">Option 1</property>
                          </object>
                        </property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </property>
        <property name="sidebar">
          <object class="AdwNavigationPage">
            <child>
              <object class="AdwToolbarView">
                <child type="top">
                  <object class="AdwHeaderBar"/>
                </child>
                <child>
                  <object class="GtkListBox" id="sidebar_list">
                    <child>
                      <object class="AdwActionRow">
                        <property name="title">Summary</property>
                      </object>
                    </child>
                    <child>
                      <object class="AdwActionRow">
                        <property name="title">Option 1</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </property>
      </object>
    </child>
  </object>
